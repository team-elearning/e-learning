{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.json",
    // "src/components/shared/Sidebar.vue",
    "@/components/shared/Sidebar.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
